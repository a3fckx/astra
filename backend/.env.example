# Template for .env (copy to .env and fill)
MONGODB_USERNAME=your_db_username
MONGODB_PASSWORD=your_db_password
MONGODB_CLUSTER=astra.ethgite  # From your URI
# Optional full URI (overrides username/password if set)
# MONGODB_URI=mongodb+srv://USERNAME:PASSWORD@astra.ethgite.mongodb.net/?retryWrites=true&w=majority&appName=astra
ELEVENLABS_API_KEY=your_api_key

GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_REDIRECT_URI=http://localhost:8000/auth/google/callback
GOOGLE_ENABLE_BIRTHDAY_SCOPE=true
GOOGLE_ENABLE_GMAIL_READ_SCOPE=false
GOOGLE_INCLUDE_GRANTED_SCOPES=true
GOOGLE_ACCESS_TYPE=offline
GOOGLE_PROMPT=consent
